#include <iostream>
#include <string>
#include <sstream>
using namespace std;


int main()
{
	string str;
	cout << "enter words " << endl;
    getline(cin, str);

    stringstream stream(str); // для розбиття введення на окремі слова
    string words;
    string result;
    while (stream >> words) {
      
            result += words.substr(1) + words[0] + ' '; // повертає вказану кількість символів з індекса
        }
      
        cout << result << "." << endl;
 }
